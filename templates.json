[

 {
  "Name": "SIDEV2",
  "AdminOnly": false,
  "ExternalGroups": "RBA",
  "Project": "sirius",
  "Image": "was_liberty:latest",
  "Env": [
   "HFS_HOME=/app/si/dev2",
   "HFS_HOME_PKG=/app/pkg/dev",
   "APP_ROOT=/mnt/si",
   "PKG_ROOT=/mnt/pkg",
   "CODE_PATH=/app/si/tp3/code/code.tar.gz",
   "BATCH_SHELL_LOCATION=/mnt/pc_data/sirius-batch/src/main/resources/batch",
   "BATCH_RESOURCE_LOCATION=/mnt/pc_data/sirius-batch/src/main/resources/hr",
   "JAVA_HOME=/opt/ibm/java/jre/",
   "LC_ALL=C.UTF-8",
   "HEALTH_CHECK_PATH=http://localhost:10000/SiDev/"
  ],
  "HostConfig": {
   "NetworkMode": "macvlan0",
   "Binds": [
    "/mnt/nfs_n42:/app/",
    "/bin4liberty:/usr/local/sbin/",
    "/home:/tmp/home"
   ]
  },
  "Labels": {
   "Environment": "SIDEV2"
  }
 },

